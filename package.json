{
	"name": "medium-base-nodejs",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"test": "jest --runInBand --detectOpenHandles",
		"build": "rm -rf ./dist && tsc",
		"start": "node dist/index.js",
		"migration:create": "sequelize-cli migration:generate --name",
		"migration:run": "sequelize-cli db:migrate",
		"migration:undo": "sequelize-cli db:migrate:undo",
		"migration:undo:all": "sequelize-cli db:migrate:undo:all",
		"seed:create": "sequelize-cli seed:generate --name",
		"seed:run": "sequelize-cli db:seed:all",
		"seed:undo": "sequelize-cli db:seed:undo",
		"seed:runfile": "sequelize-cli db:seed --seed",
		"tslint": "tslint --fix -c tslint.json -p tsconfig.json",
		"format": "prettier --write ./src ./db",
		"dev": "nodemon"
	},
	"author": "",
	"license": "ISC",
